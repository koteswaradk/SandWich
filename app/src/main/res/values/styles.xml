    <style name="DialogUplatform" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:background">#00000000</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:textAppearance">?android:attr/textAppearanceInverse</item>
        <item name="android:textColor">#ff00bfe1</item>
         <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="TitleBar_style">
        <item name="android:background">@drawable/header</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#FFFFFFFF</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">30sp</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>

    <style name="TitleBar_style_large">
        <item name="android:background">@drawable/header</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">70dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:gravity">center</item>
         <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">#FFFFFFFF</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">40sp</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>

    <style name="TitleBar_style_search">
        <item name="android:background">#E29334</item>
         <item name="android:fontFamily">sans-serif</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#E29334</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="Spinner_Style">
         <item name="android:fontFamily">sans-serif</item>
        <item name="android:textAppearance">?android:attr/textAppearanceInverse</item>
        <item name="android:textSize">20sp</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:background">@drawable/dropdown</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:weightSum">1</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">30dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center</item>
